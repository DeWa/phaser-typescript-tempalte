{
    "name": "phaser-typescript-template",
    "version": "1.0.0",
    "description": "Phaser 3 project template using Typescript",
    "main": "src/index.ts",
    "devDependencies": {
        "eslint-config-prettier": "^7.2.0",
        "husky": "^4.3.8",
        "lint-staged": "^10.5.3",
        "parcel-bundler": "^1.3.1",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3"
    },
    "scripts": {
        "build": "npx parcel build ./src/static/index.html",
        "start": "npx parcel serve  ./src/static/index.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DeWa/phaser-typescript-template.git"
    },
    "keywords": [
        "phaser",
        "typescript",
        "template",
        "project"
    ],
    "author": "Joonas Reinikka <joonas.reinikka@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DeWa/phaser-typescript-template/issues"
    },
    "homepage": "https://github.com/DeWa/phaser-typescript-template#readme",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "dependencies": {
        "phaser": "^3.52.0"
    }
}
